msgid "plugins.themes.material.name"
msgstr "Tema Material"

msgid "plugins.themes.material.description"
msgstr "Este tema implementa el tema Material."

msgid "plugins.themes.material.option.typography.label"
msgstr "Tipografía"

msgid "plugins.themes.material.option.typography.description"
msgstr "Elija una combinación de fuentes que se adapte a esta revista."

msgid "plugins.themes.material.option.typography.notoSans"
msgstr "Noto Sans: Una fuente digital nativa diseñada por Google para un extenso soporte de lenguaje."

msgid "plugins.themes.material.option.typography.notoSerif"
msgstr "Noto Serif: Una variante serif de Google de la fuente nativa digital."

msgid "plugins.themes.material.option.typography.notoSerif_notoSans"
msgstr "Noto Serif/Noto Sans: Una combinación complementaria con encabezados serif y cuerpo de texto sans-serif."

msgid "plugins.themes.material.option.typography.notoSans_notoSerif"
msgstr "Noto Sans/Noto Serif: Una combinación complementaria con encabezados sans-serif y cuerpo de texto serif."

msgid "plugins.themes.material.option.typography.lato"
msgstr "Lato: Una popular fuente sans-serif moderna."

msgid "plugins.themes.material.option.typography.lora"
msgstr "Lora: Una amplia gama de fuentes serif, buena para leer en línea."

msgid "plugins.themes.material.option.typography.lora_openSans"
msgstr "Lora/Open Sans: Una combinación complementaria con encabezados serif y cuerpo de texto sans-serif."

msgid "plugins.themes.material.option.colour.label"
msgstr "Color"

msgid "plugins.themes.material.option.colour.description"
msgstr "Seleciona el color del encabezado."

msgid "plugins.themes.material.option.showDescriptionInJournalIndex.option"
msgstr "Mostrar el resumen de la revista en la página de inicio."

msgid "plugins.themes.material.option.useHomepageImageAsHeader.label"
msgstr "Imagen de Fondo del Encabezado"

msgid "plugins.themes.material.option.useHomepageImageAsHeader.description"
msgstr "Cuando se haya cargado una imagen de la página de inicio, mostrar en el fondo del encabezado en lugar de su posición habitual en la página de inicio."

msgid "plugins.themes.material.option.useHomepageImageAsHeader.option"
msgstr "Mostrar la imagen de la página de inicio como fondo del encabezado."

msgid "plugins.themes.material.makeSubmission"
msgstr "Enviar Artículo"

msgid "plugins.themes.material.option.disablePrefixAndTitle.label"
msgstr "Desabilitar prefijo del título del artículo."

msgid "plugins.themes.material.option.disablePrefixAndTitle.description"
msgstr "Desabilitar prefijo del título del artículo."

msgid "plugins.themes.material.option.disablePrefixAndTitle.option"
msgstr "Desabilitar prefijo del título del artículo."

msgid "plugins.themes.material.option.disableArticleSubtitle.label"
msgstr "Desabilitar subtítulo del artículo."

msgid "plugins.themes.material.option.disableArticleSubtitle.description"
msgstr "Desabilitar subtítulo del artículo."

msgid "plugins.themes.material.option.disableArticleSubtitle.option"
msgstr "Desabilitar subtítulo del artículo."
